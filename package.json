{
  "name": "suomifi-design-system",
  "private": true,
  "description": "Suomi.fi Design System",
  "version": "0.1.0",
  "author": "Jari Porola <jari.porola@gmail.com>",
  "dependencies": {
    "@csstools/normalize.css": "^9.0.1",
    "@emotion/core": "^10.0.6",
    "@wapps/gatsby-plugin-i18next": "^1.1.3",
    "gatsby": "^2.0.91",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-emotion": "^4.0.3",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-ts-loader": "^0.1.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.8",
    "i18next": "^14.0.1",
    "jsx-to-string": "^1.4.0",
    "polished": "^2.3.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-i18next": "8",
    "react-syntax-highlighter": "^10.1.2",
    "suomifi-ui-components": "^0.0.4"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn run develop",
    "lint:code": "eslint \"src/**/*.{js,jsx,ts,tsx,md}\"",
    "format:code": "prettier-eslint --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:style": "stylelint \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format:style": "prettier-stylelint --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "validate": "yarn run lint:code && yarn run lint:style && yarn run test",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn validate"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md}": [
      "eslint"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,scss}": [
      "stylelint"
    ]
  },
  "devDependencies": {
    "@types/i18next": "^12.1.0",
    "@types/react": "^16.7.19",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/react-i18next": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "gatsby-plugin-eslint": "^2.0.3",
    "gatsby-plugin-stylelint": "^3.1.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "prettier": "^1.15.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.3",
    "stylelint": "^9.10.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5"
  },
  "repository": {
    "type": "git",
    "url": "github:vrk-kpa/suomifi-design-system"
  },
  "bugs": {
    "url": "https://github.com/vrk-kpa/suomifi-design-system/issues"
  }
}
